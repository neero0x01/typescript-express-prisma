// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./deb.db"
}

model Product {
  id String @id @default(cuid())
  name String
  description String
  price Int
}
